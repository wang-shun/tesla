<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white">
    <TextView
        android:id="@+id/tv_dishKinds"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_spacing60"
        android:background="@color/white"
        android:gravity="center"
        android:paddingTop="@dimen/item_spacing10"
        android:paddingBottom="@dimen/item_spacing10"
        android:textColor="@drawable/selector_color_white_golden"
        android:textSize="@dimen/str_defaul_20"
        />

    <TextView
        android:id="@+id/tv_bottom_line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_spacing1"
        android:layout_alignParentBottom="true"
        android:background="@color/transparent"/>



</RelativeLayout>